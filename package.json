{
  "author": "snelling-a",
  "description": "Create song notes from Spotify links with metadata and album art.",
  "devDependencies": {
    "@types/node": "24.10.1",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "@vitest/coverage-v8": "4.0.10",
    "builtin-modules": "5.0.0",
    "esbuild": "0.27.0",
    "happy-dom": "20.0.10",
    "markdownlint-cli2": "0.19.0",
    "obsidian": "latest",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "vitest": "4.0.10"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "spotify",
    "music"
  ],
  "license": "Unlicense",
  "main": "main.js",
  "name": "obsidian-song-of-the-day",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "dev": "node esbuild.config.mjs",
    "markdown:check": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "markdown:fix": "markdownlint-cli2 --fix \"**/*.md\" \"#node_modules\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "version": "1.0.3"
}
